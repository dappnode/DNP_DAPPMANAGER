{
  "name": "@dappnode/dappmanager",
  "version": "0.1.0",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node src/index",
    "dev:local": "ts-node -r dotenv/config src/index",
    "build": "webpack",
    "watch-ts": "tsc -w",
    "file": "node",
    "test": "npm run test:file \"./{,!(node_modules)/**}/*.test.ts\" ",
    "test:file": "TEST=true mocha -r ts-node/register --exit ",
    "test:int": "npm run test:int:file \"./{,!(node_modules)/**}/*.test.int.ts\" ",
    "test:int:file": "mocha -r dotenv/config -r ts-node/register --timeout 120000 --exit",
    "test:all": "npm run test && npm run test:int",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint . --ext .ts --fix",
    "prettier": "prettier --write 'src/**/*.*' 'test/**/*.*'",
    "pre-commit": "npm run build && npm run test",
    "clean": "rm -rf DNCORE/ dnp_repo/ .temp-transfer/ cache/ dist/"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "ajv": "^6.10.2",
    "async": "^2.6.1",
    "async-retry": "^1.3.1",
    "bitcoin-core": "^2.0.0",
    "cids": "^0.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "data-uri-to-buffer": "^2.0.0",
    "deepmerge": "^2.2.1",
    "dockerode": "^2.5.8",
    "ethers": "^4.0.38",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "http-proxy": "^1.18.0",
    "ipfs-http-client": "^44.0.1",
    "is-ip": "^3.0.0",
    "is-ipfs": "^1.0.3",
    "it-to-stream": "^0.1.1",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.19",
    "lowdb": "^1.0.0",
    "memoizee": "^0.4.14",
    "mime-types": "^2.1.27",
    "monero-rpc": "^0.5.0",
    "multicodec": "^1.0.1",
    "multihashes": "^0.4.18",
    "node-fetch": "^2.6.0",
    "node-os-utils": "^1.2.2",
    "semver": "^5.5.0",
    "socket.io": "^2.3.0",
    "source-map-support": "^0.5.19",
    "stack-trace": "^0.0.10",
    "tweetnacl": "^1.0.1",
    "tweetnacl-util": "^0.15.0"
  },
